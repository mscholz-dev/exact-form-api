{
  "name": "exact-form-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf ./dist && npx prisma generate",
    "predev": "rm -rf ./dist",
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q ./dist/app.js\"",
    "test": "npx prisma generate && jest --no-cache --verbose --coverage --detectOpenHandles --config ./dist/tests/config/jest.config.js",
    "git:deploy": "git checkout prod && git merge main && git push && git checkout main",
    "prisma:db:push:all": "cross-env MONGO_DB_NAME=\"exact_form_test\" npx prisma db push && cross-env MONGO_DB_NAME=\"exact_form_dev\" npx prisma db push && cross-env MONGO_DB_NAME=\"exact_form\" npx prisma db push",
    "prisma:generate:all": "cross-env MONGO_DB_NAME=\"exact_form_test\" npx prisma generate && cross-env MONGO_DB_NAME=\"exact_form_dev\" npx prisma generate && cross-env MONGO_DB_NAME=\"exact_form\" npx prisma generate",
    "prisma:all": "npm run prisma:db:push:all && npm run prisma:generate:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mscholz-dev/exact-form-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mscholz-dev/exact-form-api/issues"
  },
  "homepage": "https://github.com/mscholz-dev/exact-form-api#readme",
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "argon2": "^0.30.3",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.8.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/ip": "^1.1.0",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/supertest": "^2.0.12",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.9.0",
    "request": "^2.88.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
